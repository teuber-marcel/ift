{
    "stdev_factor": 0.01,
    "nlayers": 3,
    "apply_intrinsic_atrous": false,
    "layer1": {
	    "conv": {
            "kernel_size": [3, 3, 0],
            "nkernels_per_marker": 8,
            "dilation_rate": [1, 1, 0],
            "nkernels_per_image": 8 ,
            "stride": 1,
	        "noutput_channels": 8 
	    },
	    "relu": true,
        "pooling": {
            "type" : "max_pool",
            "size": [3, 3, 0],
            "stride": 2
        }
    },
    "layer2": {
        "conv": {
            "kernel_size": [3, 3, 0],
            "nkernels_per_marker": 16,
            "dilation_rate": [1, 1, 0],
            "nkernels_per_image": 8,
            "stride": 1,
	        "noutput_channels": 8 
	    },
	    "relu": true,
        "pooling": {
            "type" : "max_pool",
            "size": [3, 3, 0],
            "stride": 2
        }
    },
    "layer3": {
        "conv": {
            "kernel_size": [3, 3, 0],
            "nkernels_per_marker": 32,
            "dilation_rate": [1, 1, 0],
            "nkernels_per_image": 8,
            "stride": 1,
	        "noutput_channels": 8
	    },
	    "relu": true,
        "pooling": {
            "type" : "max_pool",
            "size": [3, 3, 0],
            "stride": 2
        }
    }
}