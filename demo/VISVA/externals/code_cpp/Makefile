$(OBJ)/dynamicid.o: $(SRC)/dynamicid.cpp $(HDR)/dynamicid.h
	$(CXX) $(CC_FLAGS) $(OLDIFT_INCLUDE) $(OPF_INCLUDE) $(CODE_C_INCLUDE) \
	-c $(SRC)/dynamicid.cpp -I$(HDR) -o $(OBJ)/dynamicid.o

